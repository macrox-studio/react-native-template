{
    "name": "MACROX",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "prebuild-ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios/",
        "prebuild-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
        "build-android-bundle-release": "cd android && ./gradlew bundleRelease",
        "build-android-release": "cd android && ./gradlew assembleRelease",
        "android-adb-reverse": "adb reverse tcp:8081 tcp:8081",
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "test": "jest",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "check-update": "ncu --packageFile package.json",
        "upgrade-deps": "ncu --packageFile package.json -u -p yarn"
    },
    "dependencies": {
        "react": "16.13.1",
        "nanoid": "^3.1.20",
        "lodash": "^4.17.20",
        "moment": "^2.29.1",
        "core-js": "^3.8.2",
        "i18next": "^19.8.4",
        "react-native": "^0.63.4",
        "react-i18next": "^11.8.5",
        "easy-promise-queue": "^0.5.7",
        "react-native-config": "^1.4.1",
        "react-context-theming": "^1.0.0-alpha.842",
        "react-native-localize": "^2.0.1",
        "react-native-animatable": "^1.3.3",
        "react-native-navigation": "^7.8.0",
        "react-native-device-info": "^7.3.1",
        "react-native-sensitive-info": "^5.5.8",
        "react-native-gesture-handler": "^1.9.0",
        "react-native-haptic-feedback": "^1.11.0",
        "@react-native-community/hooks": "^2.6.0",
        "react-native-navigation-hooks": "^6.2.0",
        "react-native-safe-area-context": "^3.1.9",
        "@react-native-community/netinfo": "^5.9.9",
        "@react-native-community/async-storage": "^1.12.1"
    },
    "devDependencies": {
        "jest": "^26.6.3",
        "eslint": "^7.17.0",
        "prettier": "^2.2.1",
        "typescript": "^4.1.3",
        "babel-jest": "^26.6.3",
        "@babel/core": "^7.12.10",
        "@types/jest": "^26.0.19",
        "@types/lodash": "^4.14.167",
        "@babel/runtime": "^7.12.5",
        "npm-check-updates": "^10.2.5",
        "react-test-renderer": "16.13.1",
        "@types/react-native": "^0.63.43",
        "eslint-plugin-prettier": "^3.3.1",
        "redux-devtools-extension": "^2.13.8",
        "@typescript-eslint/parser": "^4.12.0",
        "@types/react-test-renderer": "16.9.4",
        "@react-native-community/cli": "^4.13.1",
        "metro-react-native-babel-preset": "^0.64.0",
        "@typescript-eslint/eslint-plugin": "^4.12.0",
        "@react-native-community/eslint-config": "^2.0.0",
        "babel-plugin-transform-remove-console": "^6.9.4"
    },
    "resolutions": {
        "react": "^16.13.1",
        "moment": "^2.29.1",
        "lodash": "^4.17.20",
        "core-js": "^3.8.2",
        "react-native": "^0.63.4"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
